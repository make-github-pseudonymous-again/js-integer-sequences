{
  "name": "@aureooms/js-integer-sequences",
  "description": "Integer sequences for JavaScript",
  "version": "0.0.1",
  "author": "aureooms",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-integer-sequences/issues"
  },
  "dependencies": {
    "@aureooms/js-error": "^3.0.0",
    "@aureooms/js-fibonacci": "^3.1.0",
    "@aureooms/js-itertools": "^3.2.1"
  },
  "devDependencies": {
    "@aureooms/js-number": "^3.0.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "codeclimate-test-reporter": "^0.4.1",
    "coveralls": "^2.13.0",
    "esdoc": "^0.5.2",
    "nyc": "^10.2.0"
  },
  "homepage": "https://aureooms.github.io/js-integer-sequences",
  "keywords": [
    "computer-science",
    "integer-sequences",
    "mathematics"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-integer-sequences"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava ./test/src"
  }
}
